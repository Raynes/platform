<%=tr("Page {num} of {count}", "", {:num => apps.current_page, :count => apps.num_pages})%> 
&nbsp;
<%=link_to_function('<<', "goPage('#{element_id}', '#{apps.current_page-1}')", :class=>"button super #{apps.first_page? ? 'gray' : 'blue'} small") %>
<%=link_to_function('>>', "goPage('#{element_id}', '#{apps.current_page+1}')", :class=>"button super #{apps.last_page? ? 'gray' : 'blue'} small") %>
